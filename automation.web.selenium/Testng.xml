<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Suite1" verbose="1">
	<test name="Regression testing with Chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="seleniumPack.LoginFunctionality" />
